	"version": "0.2.0",
	"configurations": [
	
		{
			"name": "(gdb) Train",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/newral.out",
			"args": ["cfg/conv_test.cfg","cfg/conv_test.json","cfg/conv_test.input"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}